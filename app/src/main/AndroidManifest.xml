<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mobilekiosk">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".Manager_Function"></activity>
        <activity android:name=".StoreManageActivity" />
        <activity android:name=".AdminActivity" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyAm1T0QNrX1rO6tWyMMMK1zcbR9FmihnG0" />

        <activity android:name=".TakeoutChooseActivity" />
        <activity android:name=".OrderlistActivity" />
        <activity android:name=".OrderChooseActivity" />
        <activity android:name=".PaymentChoice" />
        <activity android:name=".PayHistory" />
        <activity android:name=".Order_Menu" />
        <activity android:name=".MapsActivity" />
        <activity android:name=".Order_Hojupmong" />
        <activity android:name=".Order_Jongro" />
        <activity
            android:name=".SplashActivity"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegisterActivity" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>